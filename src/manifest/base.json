{
    "name": "Facebook Auto Unmute Video",
    "version": "placeholder",
    "description": "Automatically unmute full screen videos shared in messages",
    "content_scripts": [
        {
            "matches": [
                "https://*.messenger.com/*",
                "https://*.facebook.com/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
